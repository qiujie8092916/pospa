{
  "plugins": ["vue"],
  "extends": ["elemefe"],
  /**
   *  "off" 或 0 - 关闭规则
   *  "warn" 或 1 - 开启规则，使用警告级别的错误：warn (不会导致程序退出),
   *  "error" 或 2 - 开启规则，使用错误级别的错误：error (当被触发的时候，程序会退出)
   */
  "rules":
  {
    "eol-last": 0,
    "no-eval": 0,
    "no-extend-native": 0,
    "indent": ["off", 2],
    "space-before-function-paren": 0
  }
}